{
  "name": "spryker/ramsey-uuid",
  "type": "library",
  "description": "RamseyUuid module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "ramsey/uuid": "^3.8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
